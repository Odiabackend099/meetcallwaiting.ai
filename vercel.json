{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "onboarding.html",
      "use": "@vercel/static"
    },
    {
      "src": "demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "dashboard.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/onboarding",
      "dest": "/onboarding.html"
    },
    {
      "src": "/demo",
      "dest": "/demo.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/how-it-works",
      "dest": "/index.html"
    },
    {
      "src": "/solutions",
      "dest": "/index.html"
    },
    {
      "src": "/pricing",
      "dest": "/index.html"
    },
    {
      "src": "/legal/privacy",
      "dest": "/index.html"
    },
    {
      "src": "/legal/terms",
      "dest": "/index.html"
    },
    {
      "src": "/support",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|webp)",
      "dest": "/$1.$2"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}