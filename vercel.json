{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "onboarding.html",
      "use": "@vercel/static"
    },
    {
      "src": "business-dashboard.html",
      "use": "@vercel/static"
    },
    {
      "src": "verify-email.html",
      "use": "@vercel/static"
    },
    {
      "src": "reset-password.html",
      "use": "@vercel/static"
    },
    {
      "src": "how-it-works.html",
      "use": "@vercel/static"
    },
    {
      "src": "solutions.html",
      "use": "@vercel/static"
    },
    {
      "src": "pricing.html",
      "use": "@vercel/static"
    },
    {
      "src": "demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "support.html",
      "use": "@vercel/static"
    },
    {
      "src": "legal/privacy.html",
      "use": "@vercel/static"
    },
    {
      "src": "legal/terms.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/onboarding",
      "destination": "/onboarding.html"
    },
    {
      "source": "/dashboard",
      "destination": "/business-dashboard.html"
    },
    {
      "source": "/verify-email",
      "destination": "/verify-email.html"
    },
    {
      "source": "/reset-password",
      "destination": "/reset-password.html"
    },
    {
      "source": "/how-it-works",
      "destination": "/how-it-works.html"
    },
    {
      "source": "/solutions",
      "destination": "/solutions.html"
    },
    {
      "source": "/pricing",
      "destination": "/pricing.html"
    },
    {
      "source": "/demo",
      "destination": "/demo.html"
    },
    {
      "source": "/support",
      "destination": "/support.html"
    },
    {
      "source": "/legal/privacy",
      "destination": "/legal/privacy.html"
    },
    {
      "source": "/legal/terms",
      "destination": "/legal/terms.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://your-api-domain.com/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard.html",
      "destination": "/dashboard",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https:; frame-ancestors 'none';"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|webp|mp3|wav)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "cleanUrls": true,
  "trailingSlash": false
}